    "win:migrate": "node node_modules\\truffle\\build\\cli.bundled.js migrate",
    "win:migrate:reset": "node node_modules\\truffle\\build\\cli.bundled.js migrate --reset",
    "win:testrpc": "node node_modules\\ethereumjs-testrpc\\build\\cli.node.js --mnemonic \"video visa alcohol fault earth naive army senior major inherit convince electric\" --trace_request true",
    "win:console:dev": "node node_modules\\truffle\\build\\cli.bundled.js console --network development",
    "win:console:rinkeby": "node node_modules\\truffle\\build\\cli.bundled.js console --network rinkeby",
    "win:console:main": "node node_modules\\truffle\\build\\cli.bundled.js console --network main"
